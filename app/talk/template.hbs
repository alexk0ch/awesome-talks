<ol class="breadcrumb">
  <li>{{#link-to 'homepage'}}
    Welcome to EmberJS
  {{/link-to}}</li>
  <li class="active">Talk # {{model.id}} | {{model.title}}</li>
</ol>

<h2 class="talk-title">{{model.title}}</h2>
<div class="talk-content row">
  <div class="col-md-12 col-lg-12">
  {{#each section in model.content}}  
    <p>{{section}}</p>
  {{/each}}
  </div>
</div>
{{outlet}}
